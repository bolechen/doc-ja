<refentry xml:id="function.timezone-version-get" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>timezone_version_get</refname>
  <refpurpose>
   timezonedb のバージョンを取得する
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>timezone_version_get</methodname>
   <void/>
  </methodsynopsis>
  <para>
   timezonedb の現在のバージョンを返します。
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   文字列を返します。
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>timezonedb のバージョンの取得</title>
    <programlisting role="php">
<![CDATA[
<?php
echo timezone_version_get();
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
2009.7
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><link linkend="timezones">サポートするタイムゾーンの一覧</link></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

